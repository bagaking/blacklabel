{
	"name": "blacklabel",
	"displayName": "blacklabel",
	"description": "A personal markdown organizer that makes it easy to project your knowledge structure and work notes, making high-volume knowledge management possible.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:blacklabel.helloWorld",
        "onCommand:blacklabel.ask",
        "onCommand:fileExplorer.openFile",
        "onView:view-blacklabel"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "blacklabel.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "blacklabel.ask",
				"category": "blacklabel",
				"title": "Ask Question"
            },
            {
				"command": "fileExplorer.openFile",
				"category": "blacklabel",
				"title": "Oppen File"
            }
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "panelblacklabel",
					"title": "Black Label",
					"icon": "media/dep.svg"
				}
			]
		},
		"views": {
			"panelblacklabel": [
				{
					"id": "view-blacklabel",
					"name": "Black Label",
					"icon": "media/dep.svg",
					"contextualTitle": "Black Label MD Mgr"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack --config ./build/node-extension.webpack.config.js",
		"watch": "webpack --watch --config ./build/node-extension.webpack.config.js",
		"package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "yarn run test-compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mkdirp": "^1.0.1",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/rimraf": "^3.0.0",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"ts-loader": "^8.0.11",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1",
		"webpack": "^5.10.0",
		"webpack-cli": "^4.2.0"
	},
	"dependencies": {
		"front-matter": "^4.0.2",
		"mkdirp": "^1.0.4",
		"path": "^0.12.7",
		"rimraf": "^3.0.2"
	}
}
